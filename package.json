  "homepage": "https://github.com/jabrythehutt/fs-s3#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mime": "2.0.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.0.10",
    "@types/s3rver": "0.0.32",
    "@types/validator": "^13.0.0",
    "aws-sdk": "^2.688.0",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
