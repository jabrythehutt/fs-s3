{
  "name": "@jabrythehutt/fs-s3",
  "version": "1.0.6",
  "description": "Simplifies the application storage layer",
  "main": "lib/index.js",
  "files": [
    "README.md",
    "lib",
    "src"
  ],
  "directories": {
    "lib": "lib"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "compile": "tsc",
    "compile:check": "tsc --noEmit",
    "clean": "del-cli 'lib', 'dist'",
    "test": "nyc mocha mocha \"src/**/*.spec.ts\"",
    "postpublish": "git push origin --all; git push origin --tags",
    "postversion": "npm publish --access=public",
    "preversion": "npm test && npm run clean && npm run compile",
    "deploy": "npm version patch -m \"Bumped version to %s [skip CI]\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jabrythehutt/fs-s3.git"
  },
  "keywords": [
    "AWS",
    "S3"
  ],
  "author": "Daniel Jabry <djabry@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jabrythehutt/fs-s3/issues"
  },
  "homepage": "https://github.com/jabrythehutt/fs-s3#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mime": "2.0.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.0.10",
    "@types/s3rver": "0.0.32",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "del": "3.0.0",
    "del-cli": "3.0.0",
    "eslint": "^7.2.0",
    "mkdirp": "^1.0.4",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "s3rver": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "validator": "^13.0.0",
    "aws-sdk": "^2.688.0"
  },
  "dependencies": {
    "fp-ts": "^2.6.2",
    "mime": "2.4.4"
  },
  "peerDependencies": {
    "aws-sdk": ">=2.400.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts"
    ],
    "include": [
      "src/**"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ]
  }
}
