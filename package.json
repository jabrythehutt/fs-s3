    "tslint": "^6.1.2",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.9.3",
    "validator": "^13.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "mkdirp": "^1.0.4"
  },
  "dependencies": {
    "fp-ts": "^2.6.2",
    "mime": "2.4.4"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
